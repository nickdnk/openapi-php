{
  "name": "nickdnk/openapi-php",
  "description": "Tools for documenting PHP code in OpenAPI.",
  "keywords": [
    "openapi",
    "swagger",
    "php"
  ],
  "license": "mit",
  "authors": [
    {
      "name": "Nicolai Cornelis"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "nickdnk\\OpenAPI\\": "src/"
    }
  },
  "autoload-dev": {
    "exclude-from-classmap": [
      "/tests/"
    ],
    "psr-4": {
      "nickdnk\\OpenAPI\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.4"
  }
}
