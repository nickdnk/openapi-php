{
  "name": "nickdnk/openapi-php",
  "description": "Tools for documenting PHP code in OpenAPI.",
  "keywords": [
    "openapi",
    "swagger",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolai Cornelis"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "nickdnk\\OpenAPI\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
